<div class="feed-index-view">
  <header>
    <h3>{{name}}</h3>
  </header>
  <ul>
    {{#each item in feedItems}}
      <li>
        {{#linkTo feed_item item}}
          <h4>{{item.title}}</h4>
          <p class="snippet">{{item.contentSnippet}}</p>
        {{/linkTo}}
      </li>
    {{/each}}
  </ul>

</div>