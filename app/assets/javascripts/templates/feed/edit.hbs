<div class="feed-edit-view">

  <h3>Edit Feed</h3>

  <label>Name: <br>{{view Ember.TextField valueBinding="name"}}</label>
  <label>Url: <br>{{view Ember.TextField valueBinding="url"}}</label>

  <button {{action save}} class="btn btn-primary btn-large">Save</button>
  {{#linkTo feeds.index class="btn btn-warning btn-large"}}Cancel{{/linkTo}}

</div>